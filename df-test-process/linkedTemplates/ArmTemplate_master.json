		},
		"lsADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsADLS'"
		},
		"ls_sql_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_db'"
		},
